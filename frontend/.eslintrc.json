{
    "env": {
        "browser": true,
        "node": true,
        "es2021": true
    },
    "extends": ["plugin:react/recommended", "eslint:recommended", "plugin:prettier/recommended"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react", "react-hooks"],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off",
        "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ],
        "endOfLine": "off"
    }
}
